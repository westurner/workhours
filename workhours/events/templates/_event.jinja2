{% if wrap %}
{% extends "base.jinja2" %}
{% endif %}

{% block content %}
{# <thead>
<tr>{% for field in fields %}<th>{{ field }}</th>{% endfor %}</tr>
</thead>
#}
{% for obj in value %}
<table class="modeltable" id="{{ table_id }}">
<tbody>
    {% for field in fields %}
<tr>
    <td>{{ field }}</td>
    <td>
    {% if obj[field] and 'url' in field and obj[field].startswith('http') %}
    <a href="{{ obj[field] }}">{{ obj[field] }}</a>
    {% else %}
    <span>{{ obj[field] }}</span>
    {% endif %}
    </td>
</tr>
{% endfor %}
<tr>
    <td>source</td>
    <td>

        {{ obj.task.args['url'] }}
    </td>
</tr>
</tbody>
</table>

{# 
<pre>
{{ obj.task }} {{ obj.task.args['url'] }}
</pre>

#}

{% endfor %}
{% endblock content %}
