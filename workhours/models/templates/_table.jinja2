<table id="{{ table_id }}">
<thead>
<tr>{% for field in fields %}<th>{{ field }}</th>{% endfor %}</tr>
</thead>
<tbody>
{% for obj in value %}
<tr>
    {% for field in fields %}
    <td class="{{field}}">
    {% if 'url' in field and obj[field] and obj[field].startswith('http') %}
    <a href="{{ obj[field] }}">{{ obj[field] }}</a>
    {% else %}
    <span>{{ obj[field] }}</span>
    {% endif %}
    </td>
    {% endfor %}
</tr>
{% endfor %}
</tbody>
</table>

